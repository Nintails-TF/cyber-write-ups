---
layout: null
---
[
  {% assign writeups = site.writeups | where_exp: "item", "item.title != ''" %}
  {% assign writeups = writeups | where_exp: "item", "item.platform != ''" %}
  {% for item in writeups %}
  {
    "title": {{ item.title | jsonify }},
    "platform": {{ item.platform | jsonify }},
    "category": {{ item.category | jsonify }},
    "difficulty": {{ item.difficulty | jsonify }},
    "os": {{ item.os | default: "" | jsonify }},
    "date": {{ item.date | default: "" | jsonify }},
    "date_created": {{ item.date_created | default: item.date | default: "" | jsonify }},
    "featured": {{ item.featured | default: false | jsonify }},
    "tags": {{ item.tags | default: "" | jsonify }},
    "url": {{ item.url | prepend: site.baseurl | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]